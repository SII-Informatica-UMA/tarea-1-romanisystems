CREATE TABLE NOTIFICACION (ID BIGINT NOT NULL, ASUNTO VARCHAR NOT NULL, EMAIL BOOLEAN, ESTADO VARCHAR, MENSAJE VARCHAR NOT NULL, MOMENTOREALENVIO TIMESTAMP, PROGRAMACIONENVIO TIMESTAMP, SMS BOOLEAN, TIPO VARCHAR NOT NULL, DESTINATARIO INTEGER, PRIMARY KEY (ID))
CREATE TABLE FALLO (ID INTEGER NOT NULL, CODE INTEGER NOT NULL, MENSAJE VARCHAR NOT NULL, NOTIFICACION BIGINT, PRIMARY KEY (ID))
CREATE TABLE DESTINATARIO (ID INTEGER NOT NULL, CORREOELECTRONICO VARCHAR NOT NULL, NOMBRE VARCHAR NOT NULL, TELEFONO VARCHAR, TIPO VARCHAR NOT NULL, PRIMARY KEY (ID))
ALTER TABLE NOTIFICACION ADD CONSTRAINT FK_NOTIFICACION_DESTINATARIO FOREIGN KEY (DESTINATARIO) REFERENCES DESTINATARIO (ID)
ALTER TABLE FALLO ADD CONSTRAINT FK_FALLO_NOTIFICACION FOREIGN KEY (NOTIFICACION) REFERENCES NOTIFICACION (ID)
